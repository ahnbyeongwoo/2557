import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0;
        String m=sc.next();
        for (int i = 0; i < n; i++) {
            sum+=m.charAt(i)-'0';//입력된 문자열에 '0'의 아스키 값은 48 즉 '5'는 53이므로 53-48=5
        }
        System.out.println(sum);
        sc.close();
    }
}
