import java.util.Scanner;
import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
        String b = sc.next();
        String c = sc.next();
        String d = sc.next();
        String n=a+b;//문자열 합
        String m=c+d;

        //int i=Integer.parseInt(n);//String을 int로 형 변환
        //int j=Integer.parseInt(m);
        System.out.println(Long.parseLong(n)+Long.parseLong(m));


    }
}
