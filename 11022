import java.util.Scanner;
   
class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int C=0;
        int T=sc.nextInt();

        for (int i = 1; i <= T; i++) {
            int A=sc.nextInt();
            int B=sc.nextInt();
            C=A+B;
            System.out.println("Case #"+i+": "+ A +" "+"+ "+ B +" "+"= "+C);

        }
        sc.close();

    }
}
